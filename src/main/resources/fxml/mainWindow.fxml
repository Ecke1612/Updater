<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="250.0" spacing="10.0" stylesheets="@mainStyle.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox style="-fx-background-color: rgb(45,45,45);">
         <children>
            <Label prefHeight="27.0" prefWidth="192.0" text="ED Launcher" textFill="#dadada">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="5.0" left="8.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="btn_settings" mnemonicParsing="false" onAction="#showSettings" style="-fx-background-color: transparent;" text="" textFill="#dadada">
               <HBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </HBox.margin>
               <font>
                  <Font name="Segoe MDL2 Assets" size="18.0" />
               </font>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <ScrollPane hbarPolicy="NEVER" prefHeight="320.0" prefWidth="250.0">
         <content>
            <VBox fx:id="vbox_apps" prefWidth="250.0" spacing="5.0" />
         </content>
      </ScrollPane>
      <HBox spacing="10.0">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
         <children>
            <Label text="© Eike Dreyer" />
            <Label fx:id="label_version" />
         </children>
      </HBox>
   </children>
</VBox>
